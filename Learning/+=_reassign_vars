# += - for reassigning variables

name = "collie"
name += "steele"
name

people = 99
people += 1
people



people = 99
people += 1
people

people *= 3
people



people = 99
people += 1
people

people -= 10
people

people *= 3
people


greeting = "BonJour!"
name = "Merci"
greet_name = greeting + " " + name
print(greet_name)
