# Consecutive if statements

# videoGame example


num_lives = 3
health = 100

if health <= 0 :
    print('You died!')
elif health <= 10:
    print ('Warning, less than 10% health!')
elif health <= 50:
    print ('Warning, less than 50% health!')


num_lives = 3
health = 100

if health <= 0 :
    print('You died!')
elif health <= 10:
    print ('Warning, less than 10% health!')
elif health <= 50:
    print ('Warning, less than 50% health!')
